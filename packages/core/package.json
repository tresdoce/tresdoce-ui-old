{
  "name": "@tresdoce-ui/core",
  "version": "1.0.0",
  "description": "React components library",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "scripts": {
    "start": "npm run storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "npm-run-all -l -c -s clear rollup",
    "clear": "rimraf build",
    "rollup": "rollup -c",
    "storybook": "start-storybook -p 6006",
    "storybook:export": "build-storybook",
    "generate": "node ./util/create-component",
    "prepublishOnly": "npm run build",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx,.mdx src util"
  },
  "keywords": [
    "react",
    "components",
    "library"
  ],
  "author": {
    "name": "Maximiliano \"Mex\" Delgado",
    "email": "mdelgado@tresdoce.com.ar",
    "url": "https://rudemex.github.io/"
  },
  "publishConfig": {
    "registry": "http://143.198.236.101:8081/repository/npm-private/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tresdoce/tresdoce-ui.git"
  },
  "bugs": {
    "url": "https://github.com/tresdoce/tresdoce-ui/issues"
  },
  "homepage": "https://github.com/tresdoce/tresdoce-ui#readme",
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --ext .ts,.tsx,.js,.jsx",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "files": [
    "build"
  ],
  "eslintConfig": {
    "extends": "@tresdoce-toolkit"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": true,
    "tabWidth": 2,
    "printWidth": 100
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "styled-components": "^5.1.1"
  },
  "resolutions": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-env": "^7.13.12",
    "@mdx-js/loader": "^1.6.22",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@storybook/addon-a11y": "^6.1.21",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-docs": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addon-viewport": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.21",
    "@storybook/theming": "^6.1.21",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "@tresdoce-toolkit/eslint-config": "^1.0.0",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "css-loader": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.42.4",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-import-css": "^2.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "signale-logger": "^1.5.0",
    "styled-components": "^5.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1"
  }
}
